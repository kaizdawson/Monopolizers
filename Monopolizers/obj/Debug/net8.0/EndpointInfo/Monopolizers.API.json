  "components": {
    "schemas": {
      "AccessLevelEnum": {
        "enum": [
          "Basic",
          "Premium",
          "VIP"
        ],
        "type": "string"
      },
      "BusinessCode": {
        "enum": [
          "GET_DATA_SUCCESSFULLY",
          "NOT_FOUND",
          "UPDATE_SUCESSFULLY",
          "CREATE_SUCCESS",
          "INVALID_INPUT",
          "EXCEPTION"
        ],
        "type": "string"
      },
      "CreateCardDTO": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "avtImgUrl": {
            "type": "string",
            "nullable": true
          },
          "arVideoUrl": {
            "type": "string",
            "nullable": true
          },
          "category": {
            "type": "integer",
            "format": "int32"
          },
          "typeId": {
            "type": "integer",
            "format": "int32"
          },
          "defaultData": {
            "type": "string",
            "nullable": true
          },
          "accessLevel": {
            "$ref": "#/components/schemas/AccessLevelEnum"
          }
        },
        "additionalProperties": false
      },
      "CreateTypeCardDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ResponseDTO": {
        "type": "object",
        "properties": {
          "isSucess": {
            "type": "boolean"
          },
          "data": {
            "nullable": true
          },
          "businessCode": {
            "$ref": "#/components/schemas/BusinessCode"
          },
          "message": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SignInModel": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "SignUpModel": {
        "required": [
          "confirmPassword",
          "email",
          "firstName",
          "lastName",
          "password",
          "phoneNumber",
          "userName"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "minLength": 1,
            "type": "string"
          },
          "userName": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 8,
            "type": "string"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string"
          },
          "phoneNumber": {
            "minLength": 1,
            "type": "string",
            "format": "tel"
          }
        },
        "additionalProperties": false
      },
      "UpdateCardDTO": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "avtImgUrl": {
            "type": "string",
            "nullable": true
          },
          "arVideoUrl": {
            "type": "string",
            "nullable": true
          },
          "category": {
            "type": "integer",
            "format": "int32"
          },
          "typeId": {
            "type": "integer",
            "format": "int32"
          },
          "defaultData": {
            "type": "string",
            "nullable": true
          },
          "accessLevel": {
            "$ref": "#/components/schemas/AccessLevelEnum"
          },
          "cardId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "UpdateTypeCardDTO": {
        "type": "object",
        "properties": {
          "typeId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Please enter a valid token",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
